<a href="{{ Store.Url }}" class="masthead-name" target="_blank">
    <img class="masthead-logo" src="{{ Store.Logo.Src }}" alt="{{ Store.Name }}" 
		{% if Store.Logo.Width %}data-width="{{ Store.Logo.Width }}"{% endif %}
		{% if Store.Logo.Height %}data-height="{{ Store.Logo.Height }}"{% endif %}
	/>
</a>