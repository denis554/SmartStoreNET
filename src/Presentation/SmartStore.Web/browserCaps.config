      maximumRenderedPageSize="300000"
      minorversion="${minor}"
      screenBitDepth="8"
      supportsBold="true"
      supportsCss="true"
      supportsDivNoWrap="true"
      supportsFontName="true"
      supportsFontSize="true"
      supportsImageSubmit="true"
      supportsItalic="true"
